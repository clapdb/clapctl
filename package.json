{
  "name": "clapctl",
  "version": "0.1.0",
  "description": "ClapDB CLI tool for managing ClapDB deployments on AWS",
  "type": "module",
  "bin": {
    "clapctl": "./src/index.ts"
  },
  "scripts": {
    "dev": "bun run src/index.ts",
    "build": "bun run scripts/build.ts",
    "test": "bun test",
    "test:coverage": "bun test --coverage",
    "lint": "bunx @biomejs/biome check src",
    "format": "bunx @biomejs/biome format --write src"
  },
  "dependencies": {
    "@aws-sdk/client-cloudformation": "^3.0.0",
    "@aws-sdk/client-ecr": "^3.0.0",
    "@aws-sdk/client-lambda": "^3.0.0",
    "@aws-sdk/client-s3": "^3.0.0",
    "@aws-sdk/client-service-quotas": "^3.0.0",
    "@aws-sdk/client-sts": "^3.0.0",
    "@aws-sdk/credential-providers": "^3.0.0",
    "@inquirer/prompts": "^7.2.0",
    "chalk": "^5.4.0",
    "cli-progress": "^3.12.0",
    "cli-table3": "^0.6.5",
    "commander": "^12.1.0",
    "open": "^10.1.0",
    "ora": "^8.1.0",
    "yaml": "^2.7.0",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/bun": "^1.1.0",
    "@types/cli-progress": "^3.11.6",
    "aws-sdk-client-mock": "^4.1.0",
    "typescript": "^5.9.3"
  },
  "keywords": [
    "clapdb",
    "cli",
    "aws",
    "cloudformation"
  ],
  "author": "ClapDB",
  "license": "MIT"
}
